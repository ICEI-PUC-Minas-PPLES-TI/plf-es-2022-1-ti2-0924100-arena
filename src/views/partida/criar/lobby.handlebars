<header>
    <div class="flex-container">
        <div><img src="/img/logo.png" class="logo" alt=""></div>
        <h1 class="titulo">Partida</h1>
        <div><a href="/atleta/home/{{id}}"><input type="button" class="botao_sair" value="Sair"></a>
        </div>
    </div>
</header>
<main>
    <div class="informacoes">
        <p><span>Esporte:</span> {{esporte.Nome}}</p>
        <p><span>Quadra:</span> {{quadra.NomeQuadra}}</p>
        <p><span>Data:</span> {{data}}</p>
        <p><span>Horário:</span> {{horarioi}}-{{horariof}}</p>
    </div>
    <div class="partida" border="1">
        <table id="table">
            <thead class="title">
                <tr>
                    <th>{{time.Nome}}</th>
                    <th>-</th>
                </tr>
            </thead>
            {{#each atletatime}}
            <tr>
                <td>{{Nome}}</td>
                <td>-</td>
            </tr>
            {{/each}}
        </table>
    </div>
</main>
<footer class="rodape">
    <div class="container">
        <div class="row teste">
            <div class="col">
                <div>
                    <p class="estilo_letra">&copy Todos os direitos resevados</p>
                </div>
            </div>
            <div class="col">
                <div class="container rodape2">
                    <div class="col">
                        <a href="" class="estilo_letra remover_decoracao">Serviços</a>
                    </div>
                    <div class="col">
                        <a href="" class="estilo_letra remover_decoracao">Sobre nós</a>
                    </div>
                    <div class="col">
                        <a href="" class="estilo_letra remover_decoracao">Contato</a>
                    </div>
                </div>
            </div>

        </div>
    </div>
</footer>
<script>
    document.title = "Lobby"
</script>